<%= simple_form_for(@contact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :subject %>
    <%= f.input :message %>
  </div>

  <div class="form-actions">
    <%= f.submit "Send", class:"btn btn-primary btn-lg active" %>
  </div>
<% end %>
